{
  "name": "application-monitoring",
  "version": "0.0.1",
  "description": "This can be used monitor multiple applications using different configurable metrics. The app is developed using NestJS for backend and NextJS for frontend in a single repository.",
  "author": "Sharafudheen KK (sharafudheen@ispgweb.com)",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build && cd src/client && next build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "npm run copy-env && nest start --watch",
    "start:debug": "npm run copy-env && nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "copy-env": "mkdir -p dist && cp .env* dist/"
  },
  "dependencies": {
    "@headlessui/react": "^1.4.1",
    "@heroicons/react": "^1.0.4",
    "@hookform/resolvers": "^2.8.2",
    "@nestjs/cli": "^8.0.2",
