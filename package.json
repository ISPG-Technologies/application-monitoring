    "@types/compression": "^1.7.1",
    "@types/express": "4.17.13",
    "@types/morgan": "^1.9.3",
    "@types/node": "14.17.5",
    "@types/ws": "7.4.7",
    "agenda": "^3.1.0",
    "autoprefixer": "^10.3.7",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.1",
    "cache-manager": "^3.4.4",
    "cache-manager-redis-store": "^2.0.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "classnames": "^2.2.6",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
