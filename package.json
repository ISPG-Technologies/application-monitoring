    "fastify-swagger": "^4.8.3",
    "helmet": "^4.6.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.33",
    "mongoose": "^5.13.7",
    "morgan": "^1.10.0",
    "nestjs-agenda": "^2.0.0",
    "nestjs-command": "^3.0.2",
    "next": "^11.1.2",
    "next-absolute-url": "^1.2.2",
    "node-fetch": "^3.0.0",
    "nodemailer": "^6.7.0",
    "nodemailer-smtp-transport": "^2.7.4",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
